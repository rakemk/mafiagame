# Supabase public URL and anon key
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=public-anon-key

# When using OAuth over LAN, set this to the exact redirect URL registered in Supabase
# Example: VITE_SUPABASE_REDIRECT_URL=http://192.168.1.189:8080/auth/callback
VITE_SUPABASE_REDIRECT_URL=http://localhost:8080/auth/callback

# Server-side (non-public) Service Role key (do NOT check into public repos)
# SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Comma-separated list of enabled OAuth providers for client UX (google,github,facebook,apple)
VITE_ENABLED_OAUTH_PROVIDERS=google
